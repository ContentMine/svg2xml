<semanticDocument xmlns="http://www.xml-cml.org/schema/ami2">
  <documentIterator>
    <pageIterator timeout="15000" maxMbyte="5.0">
      <variable name="p.root" value="${d.outputDir}/${p.page}"/>
      <pageWriter filename="${p.root}start.svg"/>
    </pageIterator>
    <documentWriter filename="${d.outputDir}" format="htmlMenu"/>
  </documentIterator>
</semanticDocument>



